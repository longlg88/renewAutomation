<?xml version="1.0" encoding="UTF-8"?><domain xmlns="http://www.tmaxsoft.com/xml/ns/jeus" version="8.0">
   <production-mode>@@PRODUCTIONMODE@@</production-mode>
   <id>@@ID@@</id>
   <security-manager>
      <default-application-domain>SYSTEM_DOMAIN</default-application-domain>
      <security-domains>
         <security-domain>
            <name>SYSTEM_DOMAIN</name>
            <authentication>
               <repository-service>
                  <xml-file-repository>
                     <config-file>
                        <filename>accounts.xml</filename>
                        <filepath>@@DOMAINDIR@@/config/security/</filepath>
                     </config-file>
                  </xml-file-repository>
               </repository-service>
               <custom-authentication-service>
                  <classname>jeus.security.impl.atn.DefaultAuthenticationService</classname>
               </custom-authentication-service>
            </authentication>
            <authorization>
               <repository-service>
                  <xml-file-repository>
                     <config-file>
                        <filename>policies.xml</filename>
                        <filepath>@@DOMAINDIR@@/config/security/</filepath>
                     </config-file>
                  </xml-file-repository>
               </repository-service>
               <custom-authorization-service>
                  <classname>jeus.security.impl.azn.DefaultAuthorizationService</classname>
               </custom-authorization-service>
            </authorization>
            <credential-verification>
               <default-credential-verification-service>
                  <password-verification/>
               </default-credential-verification-service>
            </credential-verification>
         </security-domain>
      </security-domains>
   </security-manager>
   <admin-server-name>@@HOSTNAME@@</admin-server-name>
   <domain-backup>
      <backup-on-boot>false</backup-on-boot>
   </domain-backup>
   <system-clustering-framework>
      <heartbeat-address>@@HEARTBEAT-ADDRESS@@</heartbeat-address>
      <heartbeat-port>@@HEARTBEAT-PORT@@</heartbeat-port>
      <use-virtual-multicast>false</use-virtual-multicast>
      <leader-discovery-timeout>3000</leader-discovery-timeout>
      <failure-detection-timeout>3000</failure-detection-timeout>
      <failure-verification-timeout>3000</failure-verification-timeout>
      <max-thread-pool-size>50</max-thread-pool-size>
      <min-thread-pool-size>1</min-thread-pool-size>
   </system-clustering-framework>
   <servers>
      <server>
         <name>@@HOSTNAME@@</name>
         <node-name>@@HOST_NAME@@</node-name>
         <listeners>
            <base>base</base>
            <listener>
               <name>base</name>
               <listen-address>@@BASE-ADDRESS@@</listen-address>
               <listen-port>@@BASE-PORT@@</listen-port>
               <selectors>1</selectors>
               <use-dual-selector>false</use-dual-selector>
               <backlog>128</backlog>
               <read-timeout>30000</read-timeout>
               <reserved-thread-num>0</reserved-thread-num>
            </listener>
            <listener>
               <name>http-server</name>
               <listen-address>@@BASE-ADDRESS@@</listen-address>
               <listen-port>@@HTTP-PORT@@</listen-port>
               <selectors>1</selectors>
               <use-dual-selector>false</use-dual-selector>
               <backlog>128</backlog>
               <keep-alive-timeout>600000</keep-alive-timeout>
               <read-timeout>30000</read-timeout>
               <reserved-thread-num>0</reserved-thread-num>
            </listener>
            <listener>
               <name>jms</name>
               <listen-address>@@BASE-ADDRESS@@</listen-address>
               <listen-port>@@JMS-PORT@@</listen-port>
               <selectors>1</selectors>
               <use-dual-selector>false</use-dual-selector>
               <backlog>128</backlog>
               <read-timeout>30000</read-timeout>
               <reserved-thread-num>0</reserved-thread-num>
            </listener>
         </listeners>
         <jvm-config>
            <jvm-option>-Xmx1024m -XX:MaxPermSize=128m</jvm-option>
         </jvm-config>
         <engine-init-on-startup>true</engine-init-on-startup>
         <use-web-engine>true</use-web-engine>
         <use-ejb-engine>true</use-ejb-engine>
         <use-jms-engine>true</use-jms-engine>
         <web-engine>
            <jsp-engine>
               <java-compiler>java6</java-compiler>
               <check-included-jspfile>true</check-included-jspfile>
               <keep-generated>true</keep-generated>
               <use-in-memory-compilation>true</use-in-memory-compilation>
               <graceful-jsp-reloading>false</graceful-jsp-reloading>
               <graceful-jsp-reloading-period>30000</graceful-jsp-reloading-period>
            </jsp-engine>
            <attach-stacktrace-on-error>false</attach-stacktrace-on-error>
            <web-connections>
               <http-listener>
                  <name>http1</name>
                  <postdata-read-timeout>30000</postdata-read-timeout>
                  <max-post-size>-1</max-post-size>
                  <max-parameter-count>-1</max-parameter-count>
                  <max-header-count>-1</max-header-count>
                  <max-header-size>-1</max-header-size>
                  <max-querystring-size>8192</max-querystring-size>
                  <server-listener-ref>http-server</server-listener-ref>
                  <thread-pool>
                     <use-auto-tuning>false</use-auto-tuning>
                     <limit>0</limit>
                     <min>10</min>
                     <max>20</max>
                     <max-idle-time>300000</max-idle-time>
                     <max-queue>-1</max-queue>
                  </thread-pool>
                  <server-access-control>false</server-access-control>
               </http-listener>
               <http-listener>
                  <name>ADMIN-HTTP</name>
                  <postdata-read-timeout>30000</postdata-read-timeout>
                  <max-post-size>-1</max-post-size>
                  <max-parameter-count>-1</max-parameter-count>
                  <max-header-count>-1</max-header-count>
                  <max-header-size>-1</max-header-size>
                  <max-querystring-size>8192</max-querystring-size>
                  <server-listener-ref>base</server-listener-ref>
                  <thread-pool>
                     <use-auto-tuning>false</use-auto-tuning>
                     <limit>0</limit>
                     <min>1</min>
                     <max>32</max>
                     <max-idle-time>300000</max-idle-time>
                     <max-wait-queue>0</max-wait-queue>
                     <max-queue>-1</max-queue>
                  </thread-pool>
                  <server-access-control>false</server-access-control>
               </http-listener>
            </web-connections>
            <monitoring>
               <check-thread-pool>300000</check-thread-pool>
               <check-class-reload>300000</check-class-reload>
               <check-session>300000</check-session>
            </monitoring>
            <access-log>
               <level>INFO</level>
               <use-parent-handlers>false</use-parent-handlers>
               <formatter-class>jeus.util.logging.SimpleFormatter</formatter-class>
               <handler>
                  <file-handler>
                     <name>accessLogFileHandler</name>
                     <level>FINEST</level>
                     <enable-rotation>true</enable-rotation>
                     <valid-day>1</valid-day>
                     <buffer-size>1024</buffer-size>
                     <append>false</append>
                  </file-handler>
               </handler>
               <enable>true</enable>
               <format>default</format>
               <enable-host-name-lookup>false</enable-host-name-lookup>
            </access-log>
            <session-config>
               <timeout>30</timeout>
               <max-session-count>-1</max-session-count>
               <shared>false</shared>
               <reload-persistent>false</reload-persistent>
               <tracking-mode>
                  <cookie>true</cookie>
                  <url>false</url>
                  <ssl>false</ssl>
               </tracking-mode>
               <session-cookie>
                  <cookie-name>JSESSIONID</cookie-name>
                  <version>0</version>
                  <max-age>-1</max-age>
                  <secure>false</secure>
                  <http-only>true</http-only>
               </session-cookie>
            </session-config>
            <async-timeout-min-threads>1</async-timeout-min-threads>
            <blocked-url-patterns>
               <deny-last-space-character>true</deny-last-space-character>
               <deny-null-character>true</deny-null-character>
               <encoded-pattern>%00</encoded-pattern>
               <encoded-pattern>%23</encoded-pattern>
               <encoded-pattern>%2e</encoded-pattern>
               <encoded-pattern>%2f</encoded-pattern>
               <encoded-pattern>%5c</encoded-pattern>
               <decoded-pattern>#</decoded-pattern>
               <decoded-pattern>\</decoded-pattern>
            </blocked-url-patterns>
         </web-engine>
         <ejb-engine>
            <resolution>300000</resolution>
            <use-dynamic-proxy-for-ejb2>true</use-dynamic-proxy-for-ejb2>
            <enable-user-notify>false</enable-user-notify>
            <active-management>
               <max-blocked-thread>-1</max-blocked-thread>
               <max-idle-time>300000</max-idle-time>
            </active-management>
            <timer-service>
               <support-persistence>true</support-persistence>
               <max-retrial-count>1</max-retrial-count>
               <retrial-interval>5000</retrial-interval>
               <thread-pool>
                  <min>2</min>
                  <max>30</max>
                  <period>3600000</period>
               </thread-pool>
            </timer-service>
            <async-service>
               <thread-min>0</thread-min>
               <thread-max>30</thread-max>
               <access-timeout>300000</access-timeout>
            </async-service>
         </ejb-engine>
         <jms-engine>
            <service-config>
               <name>default</name>
               <listener-name>jms</listener-name>
               <client-limit>1000</client-limit>
               <client-keepalive-timeout>20</client-keepalive-timeout>
            </service-config>
            <engine-roll>Active</engine-roll>
            <failover-check-timeout>5</failover-check-timeout>
            <failover-check-count>0</failover-check-count>
            <thread-pool>
               <min>10</min>
               <max>20</max>
               <keep-alive-time>300</keep-alive-time>
            </thread-pool>
            <connection-factory>
               <type>nonxa</type>
               <name>ConnectionFactory</name>
               <export-name>ConnectionFactory</export-name>
               <server-selection-policy>Round-robin</server-selection-policy>
               <request-blocking-time>200</request-blocking-time>
               <reconnect-enabled>false</reconnect-enabled>
               <reconnect-period>0</reconnect-period>
               <reconnect-interval>5</reconnect-interval>
               <topic-multicast-enable>false</topic-multicast-enable>
            </connection-factory>
            <connection-factory>
               <type>xa</type>
               <name>XAConnectionFactory</name>
               <export-name>XAConnectionFactory</export-name>
               <server-selection-policy>Round-robin</server-selection-policy>
               <request-blocking-time>200</request-blocking-time>
               <reconnect-enabled>false</reconnect-enabled>
               <reconnect-period>0</reconnect-period>
               <reconnect-interval>5</reconnect-interval>
               <topic-multicast-enable>false</topic-multicast-enable>
            </connection-factory>
         </jms-engine>
         <system-logging>
            <name>jeus</name>
            <level>INFO</level>
            <use-parent-handlers>false</use-parent-handlers>
            <formatter-class>jeus.util.logging.SimpleFormatter</formatter-class>
            <handler>
               <file-handler>
                  <name>fileHandler</name>
                  <level>FINEST</level>
                  <file-name>JeusServer.log</file-name>
                  <enable-rotation>true</enable-rotation>
                  <rotation-dir>${SERVER_HOME}/logs</rotation-dir>
                  <valid-day>1</valid-day>
                  <buffer-size>1024</buffer-size>
                  <append>true</append>
               </file-handler>
            </handler>
         </system-logging>
         <log-stdout-to-raw-format>true</log-stdout-to-raw-format>
         <action-on-resource-leak>Warning</action-on-resource-leak>
         <tm-config>
            <pooling>
               <shared>
                  <reserved-thread-num>0</reserved-thread-num>
               </shared>
            </pooling>
            <active-timeout>600000</active-timeout>
            <prepare-timeout>120000</prepare-timeout>
            <prepared-timeout>60000</prepared-timeout>
            <commit-timeout>240000</commit-timeout>
            <recovery-timeout>120000</recovery-timeout>
            <incomplete-timeout>86400000</incomplete-timeout>
            <tx-log-dir>${SERVER_HOME}/.workspace/tmlog</tx-log-dir>
            <automatic-recovery>false</automatic-recovery>
         </tm-config>
         <system-thread-pool>
            <min>0</min>
            <max>100</max>
            <keep-alive-time>300000</keep-alive-time>
            <queue-size>4096</queue-size>
            <stuck-thread-handling>
               <max-stuck-thread-time>3600000</max-stuck-thread-time>
               <action-on-stuck-thread>IgnoreAndReplace</action-on-stuck-thread>
               <stuck-thread-check-period>300000</stuck-thread-check-period>
            </stuck-thread-handling>
         </system-thread-pool>
         <naming-server>
            <pooling>
               <shared>
                  <reserved-thread-num>0</reserved-thread-num>
               </shared>
            </pooling>
         </naming-server>
         <data-source-remote-lookup>false</data-source-remote-lookup>
         <class-ftp>true</class-ftp>
         <use-MEJB>false</use-MEJB>
         <jms-resource>
            <destination>
               <type>queue</type>
               <name>ExamplesQueue</name>
               <export-name>ExamplesQueue</export-name>
               <subscription-limit>1024</subscription-limit>
               <quota>128M</quota>
               <max-pending-limit>128</max-pending-limit>
               <resume-dispatch-factor>0.4</resume-dispatch-factor>
               <dead-letter-destination>JEUSMQ_DLQ</dead-letter-destination>
               <expiration-policy>Delete</expiration-policy>
            </destination>
            <destination>
               <type>topic</type>
               <name>ExamplesTopic</name>
               <export-name>ExamplesTopic</export-name>
               <subscription-limit>1024</subscription-limit>
               <quota>128M</quota>
               <max-pending-limit>128</max-pending-limit>
               <resume-dispatch-factor>0.4</resume-dispatch-factor>
               <dead-letter-destination>JEUSMQ_DLQ</dead-letter-destination>
               <expiration-policy>Delete</expiration-policy>
            </destination>
         </jms-resource>
         <auto-generated>false</auto-generated>
      </server>
      <server>
         <name>@@PROOBJECT_SERVER_NAME@@</name>
         <node-name>@@HOST_NAME@@</node-name>
         <listeners>
